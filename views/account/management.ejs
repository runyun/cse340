<h1>
    <%= title %>
</h1>

<h2>Welcome <%= locals.accountData.account_firstname %> </h2>

<%- messages() %>

<p><a href="/account/edit/<%= locals.accountData.account_id %>">Edit Account Information</a></p>

<% if(locals.accountData.account_type != 'Client') { %>
   <h3>Inventory Management</h3> 
   <p><a href="/inv/">Inventory Management</a></p>
<% } %>

<% if(locals.accountData.account_type == 'Admin') { %>
   <h3>Account Type Management</h3> 
   <p><a href="/account/updateAccountType">Account Type Management</a></p>
<% } %>
